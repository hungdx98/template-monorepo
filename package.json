    "patchedDependencies": {
      "@wallet/evm@3.1.21": "patches/@wallet__evm@3.1.21.patch",
      "@wallet/core@3.1.21": "patches/@wallet__core@3.1.21.patch",
      "@wallet/base@3.1.21": "patches/@wallet__base@3.1.21.patch",
